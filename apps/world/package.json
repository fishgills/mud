{
  "name": "@mud/world",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "nx": {
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "webpack-cli build",
          "args": [
            "node-env=production"
          ]
        },
        "configurations": {
          "development": {
            "args": [
              "node-env=development"
            ]
          },
          "production": {
            "args": [
              "node-env=production"
            ]
          }
        }
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "typecheck",
          "build"
        ],
        "options": {
          "buildTarget": "@mud/world:build",
          "runBuildTargetDependencies": false,
          "port": 9231
        },
        "configurations": {
          "development": {
            "buildTarget": "@mud/world:build:development"
          },
          "production": {
            "buildTarget": "@mud/world:build:production"
          }
        }
      }
    }
  },
  "dependencies": {},
  "devDependencies": {}
}
