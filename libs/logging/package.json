{
  "name": "@mud/logging",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./nest": {
      "types": "./dist/nest.d.ts",
      "default": "./dist/nest.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "default": "./dist/register.js"
    },
    "./nest-winston-logger": {
      "types": "./dist/nest-winston-logger.d.ts",
      "default": "./dist/nest-winston-logger.js"
    },
    "./nest-winston-module": {
      "types": "./dist/nest-winston-module.d.ts",
      "default": "./dist/nest-winston-module.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "check-types": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.6",
    "nest-winston": "^1.10.0",
    "tslib": "^2.3.0",
    "winston": "^3.13.1"
  }
}
